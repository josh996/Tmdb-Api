{
    "name": "josh996/tmdb-api",
    "description": " Laravel Package for TMDB API.",
    "keywords": ["Laravel", "tmdb", "api", "php", "wrapper", "movie", "tv", "tv show", "tvdb"],
    "authors": [
        {
            "name": "Joshua Aryadi",
            "email": "mr.josh996@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Josh996\\TmdbApi\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Josh996\\TmdbApi\\TmdbApiProvider"
            ],
            "aliases": {
                "Tmdb": "Josh996\\TmdbApi\\Facades\\Tmdb"
            }
        }
    }
}
